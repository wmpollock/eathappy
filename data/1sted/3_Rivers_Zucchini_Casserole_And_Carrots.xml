<recipe>
  <notes>From "High Country Recipes from the Chester High Ski Team", Vance Newell</notes>
  <ingredients>
    <ingredient>
      <ingredient_name>Zucchini</ingredient_name>
      <quantity>3 Cups</quantity>
      <preparation>Sliced Thinly</preparation>
    </ingredient>
    <ingredient>
      <ingredient_name>Carrots</ingredient_name>
      <quantity>2</quantity>
      <preparation>Grated</preparation>
    </ingredient>
    <ingredient>
      <ingredient_name>Small Onion</ingredient_name>
      <quantity>1</quantity>
      <preparation>Chopped</preparation>
    </ingredient>
    <ingredient>
      <ingredient_name>Chicken Soup Base</ingredient_name>
      <unknown_1>06075</unknown_1>
      <quantity>1 Can</quantity>
    </ingredient>
    <ingredient>
      <ingredient_name>Sour Cream</ingredient_name>
      <unknown_1>01056</unknown_1>
      <quantity>1/2 Cup</quantity>
    </ingredient>
    <ingredient>
      <ingredient_name>Suffing Mix</ingredient_name>
      <quantity>1/2 Pkg</quantity>
    </ingredient>
    <ingredient>
      <ingredient_name>Melted Butter</ingredient_name>
      <quantity>1/3 Cup</quantity>
    </ingredient>
  </ingredients>
  <name>3 Rivers Zucchini Casserole And Carrots</name>
  <yield_measure>Servings</yield_measure>
  <category>
    <category>Vegetable</category>
  </category>
  <preparation>Mix well and bake at 350F for 24-45 minutes.</preparation>
</recipe>
