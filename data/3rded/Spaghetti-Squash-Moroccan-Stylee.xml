<recipe>
	<name>Spaghetti Squash Moroccan Stylee</name>
	<yield_count>4</yield_count>
	<yield_measure>servings</yield_measure>
	<ingredients>
		<ingredient>
			<quantity>1</quantity>
			<ingredient_name>spaghetti squash, 3 1/2-4 lb</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>1/2 stick</quantity>
			<ingredient_name>unsalted butter</ingredient_name>
			<preparation>cubed</preparation>
		</ingredient>
		<ingredient>
			<quantity>2</quantity>
			<ingredient_name>garlic gloves</ingredient_name>
			<preparation>minced</preparation>
		</ingredient>
		<ingredient>
			<quantity>1 tsp</quantity>
			<ingredient_name>ground cumin</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>1/2 tsp</quantity>
			<ingredient_name>ground coriander</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>1/8 tsp</quantity>
			<ingredient_name>cayenne pepper</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>3/4 tsp</quantity>
			<ingredient_name>salt</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>2 tbl</quantity>
			<ingredient_name>fresh cilantro</ingredient_name>
			<preparation>chopped</preparation>
