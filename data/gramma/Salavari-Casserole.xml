  <ingredients>
    <ingredient>
      <quantity>1 lb</quantity>
      <ingredient_name>hamburger (ground chuck)</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 can</quantity>
      <ingredient_name>black olives </ingredient_name>
      <preparation>[as Pitted Olives (Ripe Olives)]</preparation>
    </ingredient>
    <ingredient>
      <quantity>1 pkg</quantity>
      <ingredient_name>noodles (bow ties)</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
