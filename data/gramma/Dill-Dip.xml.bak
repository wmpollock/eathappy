<recipe>
  <name>Dill Dip</name>
  <ingredients>
    <ingredient>
      <quantity>2/3 cup</quantity>
      <ingredient_name>mayonaise</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>2/3 cup</quantity>
      <ingredient_name>sour cream</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 Tbsp</quantity>
      <ingredient_name>lemon juice</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 Tbsp</quantity>
      <ingredient_name>dill</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>2 Tbsp</quantity>
      <ingredient_name>onion</ingredient_name>
      <preparation>minced</preparation>
    </ingredient>
    <ingredient>
      <quantity>1 Tbsp</quantity>
      <ingredient_name>parsley leaves</ingredient_name>
      <preparation>crushed</preparation>
    </ingredient>
    <ingredient>
      <quantity>1 tsp</quantity>
      <ingredient_name>Beau Monde</ingredient_name>
      <preparation></preparation>
    </ingredient>
  </ingredients>
  <preparation>[Mix ingredients together and let flavors develop in the refrigerator, 3-4 hours.]</preparation>
  <primary_category>Appetizers</primary_category>
  <secondary_categorys>
    <secondary_category>Dips</secondary_category>
  </secondary_categorys>
  <untried>1</untried>
  <source_page>0036-0037</source_page>
  <entry_app>Veronica</entry_app>
  <cdate>12/18/2005</cdate>
  <mdate>12/18/2005</mdate>
  <safe_name>Dill_Dip</safe_name>
</recipe>
