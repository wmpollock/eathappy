<recipe>
  <name>Lime Salad</name>
  <safe_name></safe_name>
  <primary_category>Desserts</primary_category>
  <untried>1</untried>
  <secondary_categories>
    <secondary_category>Jello/Jello Mold</secondary_category>
  </secondary_categories>
  <yield_count></yield_count>
  <yield_measure></yield_measure>
  <prep_time></prep_time>
  <notes>Gah.  I was fine until I got to "fruit cocktail..."</notes>
  <ingredients>
    <ingredient>
      <quantity>1 pkg</quantity>
      <ingredient_name>lime jello</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 cup</quantity>
      <ingredient_name>boiling water</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>2 Tbsp</quantity>
      <ingredient_name>lime juice</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 cup</quantity>
      <ingredient_name>cottage cheese</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/2 cup</quantity>
      <ingredient_name>mayonaise</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 1/2 cup</quantity>
      <ingredient_name>fruit cocktail</ingredient_name>
      <preparation>strained</preparation>
    </ingredient>
  </ingredients>
  <preparation>Mix together lime jello, water and lime juice and allow to cool until it starts to congeal.  Add remaining ingredients.</preparation>
