<recipe>
  <name>Tomatoes Rockafeller</name>
  <safe_name></safe_name>
  <primary_category>Salad/Dressing</primary_category>
  <untried>1</untried>
  <secondary_categories>
    <secondary_category>Inspires Curiosity</secondary_category>
    <secondary_category>Tomatoes</secondary_category>
  </secondary_categories>
  <yield_count>6</yield_count>
  <yield_measure>serving</yield_measure>
  <prep_time></prep_time>
  <notes></notes>
  <ingredients>
    <ingredient>
      <quantity>3</quantity>
      <ingredient_name>large tomatoes</ingredient_name>
      <preparation>cut in half</preparation>
    </ingredient>
    <ingredient>
      <quantity>2 Tbsp</quantity>
      <ingredient_name>minced onion</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>2 Tbsp</quantity>
      <ingredient_name>minced parsley</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 Tbsp</quantity>
      <ingredient_name>butter or margarine</ingredient_name>
      <preparation>softened</preparation>
    </ingredient>
    <ingredient>
      <quantity>3/4 cup</quantity>
      <ingredient_name>chopped fresh spinach</ingredient_name>
      <preparation>drained (about 1/12 cups fresh spinach)</preparation>
    </ingredient>
    <ingredient>
      <quantity></quantity>
      <ingredient_name>salt and pepper</ingredient_name>
      <preparation>to taste</preparation>
    </ingredient>
    <ingredient>
      <quantity>2 Tbsp</quantity>
      <ingredient_name>seasoned bread crumbs</ingredient_name>
      <preparation></preparation>
    </ingredient>
  </ingredients>
  <preparation>Place tomatoes cut-side up in a lightly oiled baking dish.  Combine onion, parsley, butter, spinach, salt, pepper and paprika; spread evenly over tomatoes.  Top with crumbs.  Bake at 375F for 15 minutes.</preparation>
