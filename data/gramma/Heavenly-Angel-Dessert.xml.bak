<recipe>
  <name>Heavenly Angel Dessert</name>
  <ingredients>
    <ingredient>
      <quantity>1 cup</quantity>
      <ingredient_name>sugar</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>3 tbsp</quantity>
      <ingredient_name>corn startch</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 1/2 cups</quantity>
      <ingredient_name>orange juice</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/2 cup</quantity>
      <ingredient_name>water</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>2 tbsp</quantity>
      <ingredient_name>lemon juice</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1</quantity>
      <ingredient_name>egg</ingredient_name>
      <preparation>beaten</preparation>
    </ingredient>
    <ingredient>
      <quantity>1 tbsp</quantity>
      <ingredient_name>butter</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/2 cup</quantity>
      <ingredient_name>whipping cream</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 loaf</quantity>
      <ingredient_name>angel's food cake</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 cup</quantity>
      <ingredient_name>sweetened coconut</ingredient_name>
      <preparation>toasted</preparation>
    </ingredient>
  </ingredients>
  <preparation><![CDATA[Combine sugar cornstarch and salt.  Add orange juice, water, lemon juice & egg.  Mix well.  Cook over medium heat stirring constantly, until thickened.  Add buttera and cool to lukewarm.  Beat cream.  Stir into mixture.  Break cake into 1 inch pieces.  Line bottom of 8 inch square pan 2 1/2 of cake pieces.  Cover with half of pudding mixture.  Layer rest of cake pieces and pudding mixture.  Layer rest of cake pieces and pudding.  Sprinkle with coconut.  Chill at lesat 2 hours.  Cut into squares and serve.]]></preparation>
  <primary_category>Desserts</primary_category>
  <secondary_categorys></secondary_categorys>
