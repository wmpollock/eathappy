<recipe>
  <name>Salmon Casserole with Caraway</name>
  <safe_name></safe_name>
  <primary_category>Casseroles/One Dish Meals</primary_category>
  <untried></untried>
  <secondary_categories/>
  <yield_count>6</yield_count>
  <yield_measure>Servings</yield_measure>
  <prep_time></prep_time>
  <notes></notes>
  <ingredients>
    <ingredient>
      <quantity>2</quantity>
      <ingredient_name>eggs</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/2 up</quantity>
      <ingredient_name>nonfat dry milk</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 tsp</quantity>
      <ingredient_name>salt</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 tsp</quantity>
      <ingredient_name>pepper</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 can (16 oz)</quantity>
      <ingredient_name>salmon</ingredient_name>
      <preparation>drained</preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 cup</quantity>
      <ingredient_name>parsley flakes</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 cup</quantity>
      <ingredient_name>onion</ingredient_name>
      <preparation>finely chopped</preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 cup</quantity>
      <ingredient_name>celery including leaves</ingredient_name>
      <preparation>sliced</preparation>
    </ingredient>
    <ingredient>
      <quantity>1 tsp</quantity>
      <ingredient_name>caraway seed</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 cup</quantity>
      <ingredient_name>shredded cheddar cheese</ingredient_name>
      <preparation></preparation>
    </ingredient>
  </ingredients>
  <preparation>Mix together eggs and dry milk until smooth.  Stir in salt, pepper, milk.  Add salmon, parsley, onion, celery and leaves + caraway seed and mix thoroughly.  Turn into 1 1/2 quart greased casserole.  Bake uncovered at 350F or until set.</preparation>
