<recipe>
  <name>Cherries and Cream Pie</name>
  <ingredients>
    <ingredient>
      <quantity>1 3oz pkg</quantity>
      <ingredient_name>cherry jellow</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>3/4 cup</quantity>
      <ingredient_name>boiling water</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 pint</quantity>
      <ingredient_name>vanilla ice-cream</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 22oz can</quantity>
      <ingredient_name>cherry pie filling</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 -10f</quantity>
      <ingredient_name>Graham cracker crust</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity></quantity>
      <ingredient_name>whipped desert topping</ingredient_name>
      <preparation></preparation>
    </ingredient>
  </ingredients>
  <preparation><![CDATA[Combine gelatin + boiling water.  Add ice cream & stir until melted.

Set aside 5-6 cherries.  Blend remainder into ice cream mixture.  Pout into pie crust.  Chill.  Top with whipped topping and cherries.]]></preparation>
