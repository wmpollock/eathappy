      <ingredient_name>chopped nuts</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 small can</quantity>
      <ingredient_name>crushed pineapple</ingredient_name>
      <preparation>drained</preparation>
    </ingredient>
    <ingredient>
      <quantity>1 cup</quantity>
      <ingredient_name>red port wine</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity></quantity>
      <ingredient_name>**Topping</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>small pkg</quantity>
      <ingredient_name>cream cheese</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 small container</quantity>
      <ingredient_name>sour cream</ingredient_name>
      <preparation></preparation>
    </ingredient>
  </ingredients>
  <preparation>Mix ingredients and allow to set in a 9x13" pan.  Mix together topping and top.</preparation>
