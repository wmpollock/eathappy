<recipe>
	<name>Almond Tabbouleh</name>
	<ingredients>
		<ingredient>
			<quantity>1 cup</quantity>
			<ingredient_name>bulgar (cracked wheat)</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>2/3 cup</quantity>
			<ingredient_name>sliced green onions</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>1/2 cup</quantity>
			<ingredient_name>chopped parsley</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>2 tsp</quantity>
			<ingredient_name>chopped mint</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>2/3 cup</quantity>
			<ingredient_name>chopped almonds</ingredient_name>
			<preparation>toasted</preparation>
		</ingredient>
		<ingredient>
			<quantity>2/3 cup</quantity>
			<ingredient_name>cherry tomatoes</ingredient_name>
			<preparation>quartered or 1/2 cup raisins</preparation>
		</ingredient>
		<ingredient>
			<quantity>1/3 cup</quantity>
			<ingredient_name>lime or lemon juice</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity>1/4 cup</quantity>
			<ingredient_name>oil</ingredient_name>
			<preparation></preparation>
		</ingredient>
		<ingredient>
			<quantity></quantity>
			<ingredient_name><![CDATA[salt & pepper to taste]]></ingredient_name>
			<preparation></preparation>
		</ingredient>
	</ingredients>
	<preparation><![CDATA[Pout boiling water over bulgar and set aside about 30 minutes until complete absorbed.  Add remaining ingredients, toss, cover & chill for at least 2 hours.  Serve on romaine leaves. (Very Good)]]></preparation>
