  <prep_time></prep_time>
  <name>Strawberry Cream Squares</name>
  <secondary_categories>
    <secondary_categorie>**New05</secondary_categorie>
    <secondary_categorie>70s Flashback</secondary_categorie>
  </secondary_categories>
  <source_date></source_date>
  <source_name>From "Sharing Our Favorites: the Dana Guild, Senshin Buddhist Church, Los Angeles, CA" -- Mrs. Kiyojo Ueda</source_name>
  <notes></notes>
  <ingredients>
    <ingredient>
      <ingredient_name>3oz pkg strawberry gelatin</ingredient_name>
      <quantity>2</quantity>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>boiling water</ingredient_name>
      <quantity>2 cup</quantity>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>10oz pkg. frozen strawberries</ingredient_name>
      <quantity>2</quantity>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>crushed pineapple</ingredient_name>
      <quantity>1 can</quantity>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>large ripe bananas</ingredient_name>
      <quantity>2</quantity>
      <preparation>finely diced</preparation>
    </ingredient>
    <ingredient>
      <ingredient_name>sour cream</ingredient_name>
      <quantity>1 cup</quantity>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>extra sour cream</ingredient_name>
      <quantity/>
      <preparation>for topping</preparation>
    </ingredient>
  </ingredients>
