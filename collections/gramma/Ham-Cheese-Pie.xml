<recipe>
  <name><![CDATA[Ham & Cheese Pie]]></name>
  <ingredients>
    <ingredient>
      <quantity></quantity>
      <ingredient_name>**Crust</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>4 cups</quantity>
      <ingredient_name>crispy corn squares (Corn Chex)</ingredient_name>
      <preparation>crushed to 1 1/2 cups</preparation>
    </ingredient>
    <ingredient>
      <quantity>1/2 Tbsp</quantity>
      <ingredient_name>caraway seeds</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 tsp</quantity>
      <ingredient_name>onion salt</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 tsp</quantity>
      <ingredient_name>dry mustard</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/3 cup</quantity>
      <ingredient_name>butter or margerine</ingredient_name>
      <preparation>melted</preparation>
    </ingredient>
    <ingredient>
      <quantity></quantity>
      <ingredient_name>**Filling</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 cup</quantity>
      <ingredient_name>finely diced ham</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>4 oz</quantity>
      <ingredient_name>shredded Swiss cheese</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>4</quantity>
      <ingredient_name>eggs</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 3/4 cup</quantity>
      <ingredient_name><![CDATA[half & half]]></ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 tsp</quantity>
      <ingredient_name>onion salt</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/4 tsp</quantity>
      <ingredient_name>dry mustard</ingredient_name>
      <preparation></preparation>
    </ingredient>
  </ingredients>
  <preparation>Mix crust mixture, stir in butter.  Press into 9 inch pi plate.  Bake at 325F for 9 minutes.  Place ham in bottom of pie pan, sprinkle with cheese.  Combine remaining ingredients.  Bake at 350F for about 35 minutes.  Cool 15 minutes.  Sprinkle with paprika.</preparation>
