<recipe>
  <name>Cherry Cobbler</name>
  <ingredients>
    <ingredient>
      <quantity>1 can</quantity>
      <ingredient_name>cherries</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1/2</quantity>
      <ingredient_name>lemon</ingredient_name>
      <preparation>juiced</preparation>
    </ingredient>
    <ingredient>
      <quantity>1 pkg</quantity>
      <ingredient_name>white cake mix</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>1 cube</quantity>
      <ingredient_name>margarine</ingredient_name>
      <preparation>melted</preparation>
    </ingredient>
    <ingredient>
      <quantity></quantity>
      <ingredient_name>chopped nuts</ingredient_name>
      <preparation></preparation>
    </ingredient>
  </ingredients>
  <preparation>Juice lemon over cherries.  Sprinkle cake mix over fruit.  Melt margarine and top cake mix with margarine and nuts.

Bake at 325F for 50 minutes.</preparation>
