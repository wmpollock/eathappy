    </ingredient>
    <ingredient>
      <quantity>1</quantity>
      <ingredient_name>medium zucchini</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
      <quantity>2 cups</quantity>
      <ingredient_name>sliced cook turkey</ingredient_name>
      <preparation></preparation>
    </ingredient>
    <ingredient>
