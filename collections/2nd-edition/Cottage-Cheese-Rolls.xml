  <ingredients>
    <ingredient>
      <ingredient_name>butter or margarine</ingredient_name>
      <quantity>1/2 lb</quantity>
      <preparation>at room temperature</preparation>
    </ingredient>
    <ingredient>
      <ingredient_name>cottage cheese</ingredient_name>
      <quantity>8 oz</quantity>
      <preparation>at room temperature</preparation>
    </ingredient>
    <ingredient>
      <ingredient_name>salt</ingredient_name>
      <quantity>1/2 tsp</quantity>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>flour</ingredient_name>
      <quantity>2 cup</quantity>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>powdered sugar</ingredient_name>
      <quantity>2 cup</quantity>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>milk or water</ingredient_name>
      <quantity/>
      <preparation/>
    </ingredient>
    <ingredient>
      <ingredient_name>coconut or chopped nuts</ingredient_name>
      <quantity/>
      <preparation/>
    </ingredient>
  </ingredients>
